    "better-sqlite3": "^9.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "electron-updater": "^6.1.7",
    "express": "^4.19.2",
    "i18next": "^23.11.4",
